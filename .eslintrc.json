{
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended", "plugin:prettier/recommended"
	],
	"globals": {
		"discordClient": true,
		"discordWebhook": true,
		"mainGuild":  true,
		"ticketsChannel":  true,
		"tickets": true,
		"threads": true,
		"mutes": true
	},
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-prototype-builtins": "off",
		"prefer-const": "error"
	}
}
